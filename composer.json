{
    "name": "stuyam/laravel-phone-validator",
    "description": "A phone validator for Laravel using the free Twilio phone lookup service.",
    "license": "MIT",
    "keywords": ["laravel", "phone", "twilio", "validator", "validation"],
    "authors": [
        {
            "name": "Stuart Yamartino",
            "email": "stuartyamartino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "twilio/sdk": "^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "StuYam\\PhoneValidator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StuYam\\PhoneValidator\\PhoneValidatorServiceProvider"
            ]
        }
    }
}
